{
  "name": "google-calendar-app",
  "version": "1.0.0",
  "description": "An app that uses Google OAuth and the Calendar API to retrieve and render out events",
  "main": "bin/start.js",
  "scripts": {
    "scss": "node-sass public/scss -o public/css",
    "scss:watch": "node-sass --watch public/scss -o public/css",
    "start": "npm run scss && node .",
    "dev": "npm run scss && nodemon ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denis-onder/google-calendar-api.git"
  },
  "keywords": [
    "Google",
    "Calendar",
    "Express",
    "Node",
    "Handlebars"
  ],
  "author": "Denis Onder",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/denis-onder/google-calendar-api/issues"
  },
  "homepage": "https://github.com/denis-onder/google-calendar-api#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "helmet": "^3.21.2",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.0"
  }
}
